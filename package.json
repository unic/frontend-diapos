{
  "name": "dev.fe.zrh.unic-weekly-presentations",
  "version": "1.0.0",
  "description": "Presentations from our Frontend weeklies",
  "scripts": {
    "start": "reveal-md ./decks/ -w --listing-template ./custom/listing.html",
    "build": "rm -rf ./build && reveal-md ./decks/ --static build --static-dirs=./decks/assets --listing-template ./custom/listing.html",
    "serve:static": "serve ./build/"
  },
  "keywords": [
    "unic",
    "frontend",
    "markdown",
    "revealjs",
    "reveal.js",
    "reveal-md",
    "slides",
    "presentations"
  ],
  "author": "Oriol Torrent Florensa",
  "license": "MIT",
  "dependencies": {
    "reveal-md": "5.0.0",
    "serve": "11.3.2"
  }
}
